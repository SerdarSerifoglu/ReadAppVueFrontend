t type=\"email\" placeholder=\"Email\" v-model=\"loginData.email\" />\n      </div>\n      <div class=\"password-div\">\n        <input\n          type=\"password\"\n          placeholder=\"Password\"\n          v-model=\"loginData.password\"\n        />\n      </div>\n      <div class=\"forgotPass\" @click=\"goForgotPassword\">Forgot my password</div>\n      <div class=\"login-div\">\n        <button @click=\"submit\">LOGIN</button>\n      </div>\n    </div>\n    <loading></loading>\n  </div>\n</template>\n\n<script>\nimport router from \"../router/index.js\";\nimport axiosNonTokenService from \"../helpers/axiosHelperNonToken.js\";\nimport Loading from \"../components/Loading.vue\";\n\nexport default {\n  data() {\n    return {\n      loginData: {\n        email: \"\",\n        password: \"\",\n      },\n      tokenNow: \"\",\n    };\n  },\n  name: \"Login\",\n  components: {\n    loading: Loading,\n  },\n  methods: {\n    submit() {\n      axiosNonTokenService\n        .post(\"/auth/login\", { ...this.loginData })\n        .then(response => {\n          this.$store.commit(\"setToken\", response.data.access_token);\n          router.push(\"/reader\");\n        })\n        .catch(e => console.log(e));\n    },\n    readToken() {\n      this.tokenNow = this.$store.state.token;\n    },\n    goForgotPassword: function() {\n      router.push(\"/forgotpassword\");\n    },\n  },\n  beforeCreate: function() {\n    localStorage.removeItem(\"vuex\");\n  },\n};\n</script>\n"],"sourceRoot":"src/views"}]}