AA,OAAA,CAAA,UAAA,OAAA,EAAA;AACA,wBACA,KAAA,CAAA,OAAA,CAAA,QAAA,CAAA,MAAA,OAAA,CAAA,QAAA,CAAA,WAAA,EAAA,GAAA,GAAA,CADA,EAEA;AACA,0BAAA,SAAA,GAAA,MAAA,OAAA,CAAA,QAAA,CAAA,WAAA,EAAA,GAAA,GAAA;AACA,0BAAA,KAAA,GAAA,IAAA,MAAA,CAAA,SAAA,EAAA,IAAA,CAAA;AACA,sBAAA,KAAA,CAAA,OAAA,GAAA,KAAA,CAAA,OAAA,CAAA,OAAA,CACA,KADA,kCAEA,SAFA,iDAEA,SAFA,kBAEA,OAAA,CAAA,aAAA,CAAA,WAAA,EAFA,oBAAA;AAIA;AACA,mBAXA;AAYA,iBArBA,EAsBA,KAtBA,CAsBA,UAAA,CAAA;AAAA,yBAAA,OAAA,CAAA,GAAA,CAAA,CAAA,CAAA;AAAA,iBAtBA,CADA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA,OADA;AA0BA,IAAA,SA1BA,uBA0BA;AACA,WAAA,QAAA,GAAA,KAAA,MAAA,CAAA,KAAA,CAAA,KAAA;AACA;AA5BA,GAfA;AA6CA,EAAA,OA7CA,qBA6CA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qBACA,KAAA,CACA,GADA,CACA,uCADA,EACA;AACA,gBAAA,OAAA,EAAA;AACA,kBAAA,aAAA,oBAAA,MAAA,CAAA,MAAA,CAAA,KAAA,CAAA,KAAA;AADA;AADA,eADA,EAMA,IANA,CAMA,UAAA,QAAA,EAAA;AACA,gBAAA,MAAA,CAAA,gBAAA,GAAA,QAAA,CAAA,IAAA,CAAA,IAAA;AACA,eARA,EASA,KATA,CASA,UAAA,CAAA;AAAA,uBAAA,OAAA,CAAA,GAAA,CAAA,CAAA,CAAA;AAAA,eATA,CADA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAWA;AAxDA,CAAA","sourcesContent":["<style>\n.tooltip {\n  border-bottom: 1px dotted #000000;\n  color: #000000;\n  outline: none;\n  cursor: help;\n  text-decoration: none;\n  position: relative;\n  color: red;\n  font-weight: bolder;\n}\n.tooltip span {\n  margin-left: -999em;\n  position: absolute;\n}\n.tooltip:hover span {\n  border-radius: 5px 5px;\n  -moz-border-radius: 5px;\n  -webkit-border-radius: 5px;\n  box-shadow: 5px 5px 5px rgba(0, 0, 0, 0.1);\n  -webkit-box-shadow: 5px 5px rgba(0, 0, 0, 0.1);\n  -moz-box-shadow: 5px 5px rgba(0, 0, 0, 0.1);\n  font-family: Calibri, Tahoma, Geneva, sans-serif;\n  position: absolute;\n  left: 1em;\n  top: 2em;\n  z-index: 99;\n  margin-left: 0;\n  width: 250px;\n}\n.tooltip:hover em {\n  font-family: Candara, Tahoma, Geneva, sans-serif;\n  font-size: 1.2em;\n  font-weight: bold;\n  display: block;\n  padding: 0.2em 0 0.6em 0;\n}\n.custom {\n  padding: 0.5em 0.8em 0.8em 2em;\n}\n.critical {\n  background: #ffccaa;\n  border: 1px solid #ffad33;\n}\n</style>\n\n<template>\n  <div class=\"container\">\n    <app-combobox\n      :mainData=\"packComboboxData\"\n      @comboboxChange=\"selectedPack = $event\"\n    />\n    <p>{{ words }}</p>\n    <div class=\"reader\">\n      <div class=\"form-group\">\n        <textarea v-if=\"openTextArea\" v-model=\"article\"></textarea>\n      </div>\n      <button @click=\"packComboboxChange\" class=\"btn btn-primary\">Add</button>\n\n      <p v-html=\"article\" v-if=\"openTextArea\"></p>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport Combobox from \"../components/Combobox.vue\";\nexport default {\n  data() {\n    return {\n      selectedPack: \"\",\n      packComboboxData: [],\n      words: [],\n      article: \"I will eat apple by\",\n      openTextArea: true,\n      word: {}\n    };\n  },\n  name: \"Edit\",\n  components: {\n    \"app-combobox\": Combobox\n  },\n  methods: {\n    packComboboxChange: async function() {\n      await axios\n        .get(\"http://localhost:5000/api/pack/\" + this.selectedPack + \"/words\", {\n          headers: {\n            Authorization: `Bearer: ${this.$store.state.token}`\n          }\n        })\n        .then(response => {\n          this.words = response.data.data[0].words;\n          console.log(this.words);\n          this.words.forEach(element => {\n            if (\n              this.article.includes(\" \" + element.mainWord.toLowerCase() + \" \")\n            ) {\n              var findValue = \" \" + element.mainWord.toLowerCase() + \" \";\n              var regex = new RegExp(findValue, \"gi\");\n              this.article = this.article.replace(\n                regex,\n                `<span class='tooltip'>${findValue}<span class=\"custom critical\"><em>${findValue}</em>${element.secondaryWord.toLowerCase()}</span></span>`\n              );\n            }\n          });\n        })\n        .catch(e => console.log(e));\n    },\n    readToken() {\n      this.tokenNow = this.$store.state.token;\n    }\n  },\n  async created() {\n    await axios\n      .get(\"http://localhost:5000/api/pack/forCbx\", {\n        headers: {\n          Authorization: `Bearer: ${this.$store.state.token}`\n        }\n      })\n      .then(response => {\n        this.packComboboxData = response.data.data;\n      })\n      .catch(e => console.log(e));\n  }\n};\n</script>\n"],"sourceRoot":"src/views"}]}